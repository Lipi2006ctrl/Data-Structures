#include <iostream>
#include <algorithm>
using namespace std;

int linearSearch(int arr[], int n, int key) {
    for (int i = 0; i < n; i++)
        if (arr[i] == key)
            return i;
    return -1;
}

int binarySearch(int arr[], int n, int key) {
    int low = 0, high = n - 1;
    while (low <= high) {
        int mid = (low + high) / 2;
        if (arr[mid] == key)
            return mid;
        else if (arr[mid] < key)
            low = mid + 1;
        else
            high = mid - 1;
    }
    return -1;
}

int main() {
    int arr[] = {10, 20, 30, 40, 50};
    int n = 5, key = 30;

    cout << "Linear Search: ";
    int index1 = linearSearch(arr, n, key);
    cout << (index1 != -1 ? "Found" : "Not Found") << endl;

    cout << "Binary Search: ";
    int index2 = binarySearch(arr, n, key);
    cout << (index2 != -1 ? "Found" : "Not Found") << endl;

    return 0;
}
