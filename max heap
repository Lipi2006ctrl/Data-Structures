#include <iostream>
#include <queue>
#include <vector>
using namespace std;

int main() {
 
    priority_queue<int> maxHeap;


    maxHeap.push(10);
    maxHeap.push(5);
    maxHeap.push(20);
    maxHeap.push(1);

    cout << "Max Heap elements (in descending order): ";
    while (!maxHeap.empty()) {
        cout << maxHeap.top() << " ";
        maxHeap.pop();                
    }
    return 0;
}
