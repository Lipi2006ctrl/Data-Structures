#include <iostream>
#include <queue>
using namespace std;

int main() {
    // Max-heap priority queue (default)
    priority_queue<int> pq;

    // Insert elements
    pq.push(10);
    pq.push(30);
    pq.push(20);
    pq.push(5);

    cout << "Elements in priority queue (max-heap): ";
    while (!pq.empty()) {
        cout << pq.top() << " "; // prints highest priority element
        pq.pop();
    }
    cout << endl;

    // Min-heap priority queue
    priority_queue<int, vector<int>, greater<int>> minPQ;
    minPQ.push(10);
    minPQ.push(30);
    minPQ.push(20);
    minPQ.push(5);

    cout << "Elements in priority queue (min-heap): ";
    while (!minPQ.empty()) {
        cout << minPQ.top() << " "; // prints smallest element first
        minPQ.pop();
    }
    cout << endl;

    return 0;
}
